{
  "name": "pharma-agent-backend",
  "version": "1.0.0",
  "description": "Pharmaceutical Agent Backend with Gemini API",
  "main": "dist/index.js",
  "scripts": {    "collect:india-patents": "ts-node scripts/scrapeIndiaPatents.ts",
    "verify:india-patents": "ts-node scripts/verifyIndiaPatents.ts",
    "download:real-data": "bash scripts/download-real-data.sh",
    "parse:orange-book": "ts-node scripts/parseOrangeBook.ts",
    "setup:real-data": "npm run download:real-data && npm run parse:orange-book",
    "validate:downloads": "ts-node scripts/validateDownloads.ts",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:seed": "ts-node prisma/seed.ts",
    "prisma:seed:real": "ts-node prisma/seedRealData.ts",
    "seed:real": "ts-node prisma/seedRealData.ts"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^6.19.0",
    "@types/cheerio": "^0.22.35",
    "axios": "^1.13.4",
    "cheerio": "^1.1.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "pdfkit": "^0.17.2",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/node": "^24.10.2",
    "@types/pdfkit": "^0.17.4",
    "nodemon": "^3.1.11",
    "prisma": "^6.19.0",
